%% -*- mode: erlang -*-

%% Targeted Sites
sites() -> ["http://hypernumbers.dev:9000"].

actions() ->
    Url  = "http://hypernumbers.dev:9000/delete/",
    Type = "application/json",
    Data = "{\"delete\":\"all\"}",
    httpc:request(post,{Url, [{"Accept", "application/json"}], Type, Data}, [], []),
    ok.
