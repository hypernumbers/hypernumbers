%% -*- mode: erlang -*-

%% Targeted Sites
sites() -> ["http://localhost:9000"].

actions() ->
    Url  = "http://localhost:9000/insert_rows/",
    Type = "application/json",
    Accept = [{"Accept", "application/json"}],
    Data = "{\"insert\":\"before\"}",
    httpc:request(post,{Url++"8:8", Accept, Type, Data}, [], []),
    httpc:request(post,{Url++"10:11", Accept, Type, Data}, [], []),
    ok.

